

Algoritmo ManejoVariables
    Definir conejos, zanahorias Como Entero
	
    conejos <- 126
    zanahorias <- 0
	
    Escribir "Contenido de la variable conejos:", conejos
	
    conejos <- 150
    Escribir "Nuevo contenido de la variable conejos:", conejos
	
    zanahorias <- conejos
	
    Escribir "Valores de zanahorias y conejos:", zanahorias, " ", conejos
	
    conejos <- 250
    Escribir "Valores actualizados:", conejos," ", zanahorias
FinAlgoritmo

	
	
	
	

